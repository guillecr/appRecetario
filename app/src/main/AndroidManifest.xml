<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.guillermo.hito">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icono"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icono2"
        android:supportsRtl="true"
        android:theme="@style/Theme.Hito">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Listado">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Listado"/>
        </activity>
        <activity android:name=".Receta">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Receta"/>
        </activity>
        <activity android:name=".AcercaDe">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".AcercaDe"/>
        </activity>
    </application>

</manifest>